{
  "name": "npmal",
  "version": "0.0.0-development",
  "description": "CLI for npm utilities.",
  "bin": "./dist/src/main.js",
  "preferGlobal": true,
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "echo 'hello world'",
    "once": "node -r ts-node/register ./src/main.ts",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Introvertuous/npmal"
  },
  "author": "Chris Ryan",
  "bugs": {
    "url": "https://github.com/Introvertuous/npmal/issues"
  },
  "homepage": "https://github.com/Introvertuous/npmal#README",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^9.4.0",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "introvertuous-configurations": "^1.6.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.5",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^3.1.6",
    "semantic-release": "^15.13.3"
  },
  "dependencies": {
    "commander": "^2.16.0",
    "npm-registry-client": "^8.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
